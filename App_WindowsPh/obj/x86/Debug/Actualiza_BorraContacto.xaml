<Page
    x:Class="App_WindowsPh.Actualiza_BorraAlumno"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App_WindowsPh"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Button x:ConnectionId='1' x:Name="button" Content="Actualiza" HorizontalAlignment="Left" Margin="149,250,0,0" VerticalAlignment="Top"                           />
        <Button x:ConnectionId='2' x:Name="button1" Content="Borra" HorizontalAlignment="Left" Margin="149,361,0,0" VerticalAlignment="Top"                           />
        <TextBox x:Name="txtnombre" HorizontalAlignment="Left" Margin="99,55,0,0" TextWrapping="Wrap" Text="Nombre" VerticalAlignment="Top" Width="239"/>
        <TextBox x:Name="txtapellidos" HorizontalAlignment="Left" Margin="99,166,0,0" TextWrapping="Wrap" Text="Apellidos" VerticalAlignment="Top" RenderTransformOrigin="-0.098,-0.002" Width="239"/>
        <Button x:Name="button2" Content="Asistencia" HorizontalAlignment="Left" Margin="149,469,0,0" VerticalAlignment="Top"/>

    </Grid>
</Page>

